<?xml version="1.0" encoding="UTF-8"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <em:id>userChromeJS@kamens.us</em:id>
    <em:type>2</em:type>
    <em:name>userChromeJS</em:name>
    <em:version>0.0.1</em:version>
    <em:creator>Jonathan Kamens</em:creator>
    <em:description>Load the contents of chrome/userChrome.js from the user's profile directory every time a window is opened.
This is an attempt to re-implement the old userChromeJS extension to make it compatible with Thunderbird 68.</em:description>
    <!--<em:homepageURL>...</em:homepageURL>-->
    <em:targetApplication>
      <Description>
        <em:id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</em:id> <!-- Thunderbird -->
        <em:minVersion>60.0</em:minVersion>
        <em:maxVersion>68.0a1</em:maxVersion>
      </Description>
    </em:targetApplication>
    <em:bootstrap>true</em:bootstrap>
  </Description>
</RDF>
